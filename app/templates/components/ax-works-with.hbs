{{#if isEmpty}}
{{yield}}
{{else}}
<div class="works-with card-panel teal">
  <div class="card-action medium center">
    <span class="card-title white-text text-darken">Recently Worked With</span>
  </div>
  <div class="people-card center">
    {{#each people key="@index" as |person|}}
     <div class="chip contact">
      <img src="{{person.avatar_url}}" alt="{{person.login}}">
      <a target="_blank" href="https://github.com/{{person.login}}">{{person.login}}</a>
     </div>
    {{/each}}
  </div>
</div>
{{/if}}
