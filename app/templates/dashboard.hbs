<div class="container">
  <div class="row">
    <div class="col s10 dashboard">
      {{ax-navbar user=user}}
      <div class="row">
        <div class="col s4">
        {{ax-profile-card user=user}}
        </div>
        <div class="col s5">
          {{ax-pull-request-card events=events loading=eventsLoading}}
        </div>
      </div>
      <div class="row">
        <div class="col s8">
          {{ax-time-chart-card events=events ranges=ranges user=user loading=eventsLoading}}
        </div>
      </div>
    </div>
    <div class="col s2 hide-on-small-only fixed range-selector">
      <ul class="table-of-contents">
        {{#each rangeData key="@index" as |range|}}
          <li><a class="{{if range.active 'active-range'}}" {{action "changeTimeView" range.daysBack}}>{{range.name}}</a></li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>
{{ax-footer}}